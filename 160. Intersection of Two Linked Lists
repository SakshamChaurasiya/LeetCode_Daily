/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) {
 *         val = x;
 *         next = null;
 *     }
 * }
 */
public class Solution {
    public ListNode getIntersectionNode(ListNode headA, ListNode headB) {
        ListNode p1 = headA,
                q1 = headB;
        while (p1 != q1) {
            p1 = (p1 == null) ? headB : p1.next;
            q1 = (q1 == null) ? headA : q1.next;
        }
        return p1;
    }
}
